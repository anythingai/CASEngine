{
  "name": "cultural-arbitrage",
  "version": "0.1.0",
  "private": true,
  "description": "Web3 trend intelligence platform that maps cultural vibes to crypto/NFT opportunities",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "start": "turbo start",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "test": "turbo test",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo build --filter=!@cultural-arbitrage/api && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "turbo": "^1.12.4",
    "typescript": "5.3.3"
  },
  "packageManager": "npm@10.2.4",
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.6.5"
  },
  "author": {
    "name": "Cultural Arbitrage Team"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/cultural-arbitrage.git"
  },
  "keywords": [
    "web3",
    "crypto",
    "nft",
    "trends",
    "cultural-arbitrage",
    "intelligence",
    "platform"
  ]
}